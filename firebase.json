{
  "hosting": {
    "public": "dist/cross-way-center-academy/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/cross-way-images/**",
        "destination": "/cross-way-images"
      },
      {
        "source": "/img/**",
        "destination": "/img"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}


  
 